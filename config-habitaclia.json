{
  "load": {
    "http": {
      "url": "https://www.habitaclia.com/comprar-vivienda-terraza_ascensor_chimenea_en-pineda_de_mar/provincia_barcelona-maresme-area_19/listainmuebles.htm"
    }
  },
  "parse": {
    "html": {
      "title": "title | trim",
      "houses": [{
        "_$": ".listainmuebles > .enlista > li.gtmlistproductview",
        "name": "h3 > a",
        "description": ".descripcion | trim",
        "price": "[itemprop='price']"
      }]
    }
  },
  "transform": {
    "json-file": {
      "path": "./habitaclia-casas.json"
    }
  }
}