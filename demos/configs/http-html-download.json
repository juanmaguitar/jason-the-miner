{
  "load": {
    "http": {
      "url": "https://www.amnesty.org/en/latest/research/2016/02/annual-report-201516/",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "reports": [{
        "_$": ".annual-report__download-dropdown .custom-select option[disabled!='disabled']",
        "_slice": "0,3",
        "lang": "< text | lowercase",
        "url": "< attr(value)"
      }]
    }
  },
  "transform": {
    "download-file": {
      "key": "url",
      "folder": "./demos/data/out/downloads",
      "concurrency": 2,
      "maxMbSize": 5
    }
  }
}
